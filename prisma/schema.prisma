generator client {
  provider = "prisma-client-js"
  previewFeatures = ["deno"]
  output = "./client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model alert_history {
  alert_id          Int     @id
  headline          String?
  short_description String?
  guid              String?
  published_to      Int?
}

model guilds {
  guild_id             BigInt   @id
  guild_name           String?
  has_alerts           Boolean?
  alert_channel        BigInt?
  accessibility_alerts Boolean?
  planned_alerts       Boolean?
  route_ids            String[]
}

model kv_store {
  key   String  @id
  value String?
}
